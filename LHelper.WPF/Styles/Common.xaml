<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:LHelper.WPF.Styles">

    <!--Colors-->


    <Color x:Key="ColorPrimary1">#2bccb9</Color>
    <Color x:Key="ColorPrimary2">#2b9ccc</Color>
    <Color x:Key="ColorWrong">#ff0800</Color>


    <!--Brushes -->
    <SolidColorBrush x:Key="BrushPrimaryKey1" Color="{StaticResource ColorPrimary1}"/>
    <SolidColorBrush x:Key="BrushPrimaryKey2" Color= "{StaticResource ColorPrimary2}"/>
    

</ResourceDictionary>